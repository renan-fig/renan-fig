#include <stdio.h>

int main(void) {
   int a, b;
   printf("Digite um numero inteiro: ");
   scanf("%d", &a);
   printf("Digite outro numero inteiro: ");
   scanf("%d", &b);
   a = a + b;
   b = a - b;
   a = a - b;
   printf("O valor final de a e b respectivamente Ã©: %d, %d", a , b);



  
 
  return 0;
}
